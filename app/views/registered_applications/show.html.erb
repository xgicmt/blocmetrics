	  <div class="stumpy" style="clear:both;">
	  <ul>
	   <% @events.each do |key, array| %>
	     <li>
	     <%= key  %>
	     <%= array.count() %>
         </li>
	    <%#= event.eventname %> <%#= event.created_at.strftime("%b %d, %Y | %H:%M:%S") %>
	  	<% end %>
	  	</ul>
	  </div>
	  <div class="tabs">
	    <div class="pieCharts">
          <%= pie_chart @registered_application.events.group(:eventname).count %>
