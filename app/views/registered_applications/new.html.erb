<h1>Track a New App</h1>
    <%= form_for @registered_application do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control', placeholder: "Enter topic name" %>
      </div>
    <div class="form-group">
      <%= f.label :url %>
      <%= f.text_field :url, class: 'form-control', placeholder: "https://www.example.com" %>
    </div>
    <%= f.submit "Save", class: 'btn btn-success' %>
    <% end %>